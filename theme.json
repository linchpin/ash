		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"black": 900
			},
			"lineHeight": {
				"one": 1,
				"heading": 1.1,
				"medium": 1.5,
				"body": 1.75
			},
			"spacing": {
				"outer": "30px"
			}
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "Gilmer-Regular, sans-serif",
