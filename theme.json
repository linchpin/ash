						{
							"fontDisplay": "block",
							"fontFamily": "Gilmer-Outline",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "normal",
							"src": [
								"file:./assets/fonts/gilmer-outline.eot",
								"file:./assets/fonts/gilmer-outline.woff2",
								"file:./assets/fonts/gilmer-outline.woff",
