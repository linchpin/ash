					"slug": "medium",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Gilmer-Medium",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/gilmer-medium.eot",
								"file:./assets/fonts/gilmer-medium.woff2",
								"file:./assets/fonts/gilmer-medium.woff",
