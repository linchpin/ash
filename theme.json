						{
							"fontDisplay": "block",
							"fontFamily": "Gilmer-Regular",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "normal",
							"src": [
								"file:./assets/fonts/gilmer-regular.eot",
								"file:./assets/fonts/gilmer-regular.woff2",
								"file:./assets/fonts/gilmer-regular.woff",
								"file:./assets/fonts/gilmer-regular.ttf",
								"file:./assets/fonts/gilmer-regular.woff2"
